		<ResourceDictionary Source="TextBoxStyle.xaml"/>
		<ResourceDictionary Source="TabControlStyle.xaml"/>
		<ResourceDictionary Source="ComboBoxStyle.xaml"/>
		<ResourceDictionary Source="TextBlockStyle.xaml"/>
		<!--<ResourceDictionary Source="ChartStyle.xaml"/>-->
		<ResourceDictionary Source="ToolTipStyle.xaml"/>
		<!--<ResourceDictionary Source="PathTrimmingTextBlockStyle.xaml"/>-->
		<ResourceDictionary Source="TreeViewStyle.xaml"/>
		<ResourceDictionary Source="GridSplitterStyle.xaml"/>
		<ResourceDictionary Source="MenuStyles.xaml"/>
		<ResourceDictionary Source="TitleStyle.xaml"/>
		<ResourceDictionary Source="SyntaxHighlighterStyles.xaml"/>
		<ResourceDictionary Source="CheckBoxStyle.xaml"/>
