	<Style TargetType="c:RadioMenuItem" BasedOn="{StaticResource RadioMenuItem}"/>
	<Style TargetType="c:HexEditor" BasedOn="{StaticResource StandardHexEditor}"/>
	<Style TargetType="c:LedIndicator" BasedOn="{StaticResource StandardLedIndicator}"/>

</ResourceDictionary>
