        <!--<ResourceDictionary Source="TabControlStyles.xaml"/>
        <ResourceDictionary Source="MenuStyles.xaml"/>-->
		<!--<ResourceDictionary Source="TextBoxStyles.xaml"/>
        <ResourceDictionary Source="ScrollBarStyles.xaml"/>
        <ResourceDictionary Source="ComboBoxStyles.xaml"/>
        <ResourceDictionary Source="GroupBoxStyles.xaml"/>-->
		<!--<ResourceDictionary Source="AppTreeStyle.xaml"/>-->

		<ResourceDictionary Source="HexEditorStyles.xaml"/>
		<ResourceDictionary Source="LedIndicatorStyle.xaml"/>
	</ResourceDictionary.MergedDictionaries>

	<Style TargetType="Window" BasedOn="{StaticResource WindowStyle}"/>
  <Style TargetType="ListBox" BasedOn="{StaticResource StandardListBox}"/>
  <Style TargetType="ScrollBar" BasedOn="{StaticResource StandardScrollBar}"/>
	<Style TargetType="c:AnnotatedScrollBar" BasedOn="{StaticResource StandardAnnotatedScrollBar}"/>
	<Style TargetType="TextBox" BasedOn="{StaticResource StandardTextBox}"/>
	<Style TargetType="Button" BasedOn="{StaticResource StandardButton}"/>
	<!--<Style TargetType="c:ReadOnlyTextBox" BasedOn="{StaticResource StandardTextBox}"/>-->
	<Style TargetType="TabControl" BasedOn="{StaticResource StandardTabControl}"/>
	<Style TargetType="TextBlock" BasedOn="{StaticResource StandardTextBlock}"/>
	<Style TargetType="ToolTip" BasedOn="{StaticResource StandardToolTip}"/>
	<!--<Style TargetType="c:PathTrimmingTextBlock" BasedOn="{StaticResource StandardPathTrimmingTextBlockStyle}"/>-->
	<Style TargetType="TreeView" BasedOn="{StaticResource StandardTreeView}"/>
	<Style TargetType="GridSplitter" BasedOn="{StaticResource StandardGridSplitter}"/>
	<Style TargetType="c:Title" BasedOn="{StaticResource StandardTitle}"/>
	<Style TargetType="DataGrid" BasedOn="{StaticResource StandardDataGrid}"/>
  <Style TargetType="ProgressBar" BasedOn="{StaticResource StandardProgressBar}"/>

	<!--<Style TargetType="c:AutoCompleteTextBox" BasedOn="{StaticResource StandardAutoCompleteTextBox}"/>-->
	<!--<Style TargetType="c:NumericUpDownInt" BasedOn="{StaticResource StandardNumericUpDown}"/>-->
